{
    "name": "compwright/docker-ec2-metadata",
    "type": "library",
    "description": "AWS EC2 metadata service simulator for Docker",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathon Hill",
            "email": "jonathon@compwright.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Compwright\\DockerEc2Metadata\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Compwright\\DockerEc2Metadata\\": "tests/"
        }
    },
    "require": {
        "php": "^8.4 || ^8.5",
        "aws/aws-sdk-php": "^3.363",
        "composer-runtime-api": "^2.2",
        "league/route": "^6.2",
        "psr/log": "^3.0",
        "react/http": "dev-patch-1",
        "react/socket": "^1.13",
        "symfony/console": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.89",
        "phpstan/phpstan": "^2.1.32"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/valzargaming/http/"
        }
    ],
    "bin": [
        "bin/docker-ec2-metadata"
    ]
}
